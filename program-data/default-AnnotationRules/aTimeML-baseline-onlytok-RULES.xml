<?xml version="1.0" ?>

<rulesets id="1">
		// view bTIMEX-baseline...
	<ruleset id="1" tag="TIMEX3" attribs="type=DATE;">
		<rule id="1" span="4">word[0]==(January|February|March|April|May|June|July|August|September|October|November|December|((Jan|Feb|Mar|Apr|May|Jun|Jul|Aug|Sep[t]?|Sept|Oct|Nov|Dec)[\.]?))(_*_)word[1]==([0-2]?[0-9]|3[01])(_*_)word[2]==,(_*_)word[3]==(1[0-9]{3}|20[12][0-9])</rule>
		<rule id="2"  span="1">word[0]==(January|February|March|April|May|June|July|August|September|October|November|December|((Jan|Feb|Mar|Apr|May|Jun|Jul|Aug|Sep[t]?|Sept|Oct|Nov|Dec)[\.]?))</rule>
